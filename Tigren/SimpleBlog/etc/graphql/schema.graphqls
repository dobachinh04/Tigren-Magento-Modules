extend type Mutation {
    createBlogCategory(input: BlogCategoryInput!): BlogCategoryOutput @resolver(class: "Tigren\\SimpleBlog\\Model\\Resolver\\CreateBlogCategory")
}

input BlogCategoryInput {
    name: String!
    description: String!
    status: Int
}

type BlogCategoryOutput {
    entity_id: Int
    name: String
    description: String
    status: Int
}
